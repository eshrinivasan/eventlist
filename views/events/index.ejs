<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'/>
</head>
<body>
<% include ../nav %>
<div class="container">
  <h1><%= title %></h1>
  <% for(var i = 0; i < data.length; i++) { %>
  <h2><%= data[i].title %></h2>

  <% include ../utils %>
  <p><%= formatDate(data[i].eventdate) %> at
    <%= data[i].venue.name %>, <%= data[i].venue.address %></p>
  <% } %>
</div>
</body>
</html>
